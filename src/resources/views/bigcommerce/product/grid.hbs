<h1>Products</h1>
<table class="table">
    <thead>
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Description</th>
    </tr>
    </thead>
    <tbody>
    {{#each products}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.description}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
